# Run a archive server of Google Storage inside virtualenv
set -eu

original=$(pwd)
cd -- "$(dirname -- "$(readlink -e -- "$0")")"
. ./find_virtualenv.sh
cd -- "$original"

exec_python_module gs_archive_server.gs_archive_server "$@"

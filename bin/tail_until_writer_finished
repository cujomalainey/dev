# Run turtle graphics inside virtualenv.
set -eu

original=$(pwd)
cd -- "$(readlink -e -- "$(dirname -- "$0")")"
. ./find_virtualenv.sh
cd -- "$original"

exec_python_module devserver_lib.tail_until_writer_finished "$@"
